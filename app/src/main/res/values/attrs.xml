<?xml version="1.0" encoding="utf-8"?>

<resources>

    <declare-styleable name="SpreadView">
        <!--中心圆颜色-->
        <attr name="spread_center_color" format="color" />
        <!--中心圆半径-->
        <attr name="spread_radius" format="integer" />
        <!--扩散圆颜色-->
        <attr name="spread_spread_color" format="color" />
        <!--扩散间距-->
        <attr name="spread_distance" format="integer" />
        <!--扩散最大半径-->
        <attr name="spread_max_radius" format="integer" />
        <!--扩散延迟间隔-->
        <attr name="spread_delay_milliseconds" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomScoreBar">
        <attr name="leftBackground" format="reference|color" />
        <attr name="rightBackground" format="reference|color" />
        <attr name="centerIcon" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="FlexBoxLayout">
        <!--默认头像-->
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="zface_round_progressBar">
        <attr name="zface_round_progressBar_zface_round_color" format="color" />
        <attr name="zface_round_progressBar_zface_round_progress_color" format="color" />
        <attr name="zface_round_progressBar_zface_text_color" format="color" />
        <attr name="zface_round_progressBar_zface_text_size" format="dimension" />
        <attr name="zface_round_progressBar_zface_round_width" format="dimension" />
        <attr name="zface_round_progressBar_zface_max" format="dimension" />
        <attr name="zface_round_progressBar_zface_text_is_displayable" format="boolean" />
        <attr name="zface_round_progressBar_zface_style" format="reference" />
        <attr name="zface_round_progressBar_zface_progress_shader" format="boolean" />
        <attr name="zface_round_progressBar_zface_color_bg_width" format="dimension" />
        <attr name="zface_round_progressBar_zface_gradient_color_start" format="color" />
        <attr name="zface_round_progressBar_zface_gradient_color_end" format="color" />
        <attr name="zface_round_progressBar_zface_start_angle" format="integer" />
        <attr name="zface_round_progressBar_zface_end_angle" format="integer" />
        <attr name="zface_round_progressBar_zface_background_color" format="color" />

    </declare-styleable>
</resources>